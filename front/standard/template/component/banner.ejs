  <div class="banner-swiper-container">
      <% if(AdsenseList&&AdsenseList.Code === 0 && AdsenseList.Result&&AdsenseList.Result.length){ %>
      <div class="swiper-wrapper">
          <%for(var i=0;i< AdsenseList.Result.length;i++){%>
            <div class="swiper-slide"><a href="<%= AdsenseList.Result[i].LinkUrl||'javascript:void(0);' %>"><img src="<%= AdsenseList.Result[i].ImageUrl %>" title="<%= AdsenseList.Result[i].ImageTitle %>"></img> </a> </div>
            <%}%>
      </div>
      <div class="pagination"></div>
      <%}%>

  </div>
