<div class="pop-inner pop-register">
  <span class="pop-register-close">
    <img src="/public/standard/vendor/images/close.png" width="30" height="30" />
  </span>
  <div class="pop-register-nav"><div class="active">注册</div></div>
  <form class="pop-register-content-sms js-register js-register-sms">
    <fieldset>
      <!-- <div class="pop-register-content-sms "> -->
      <div class="pop-register-item">
        <input type="text" name="UserName" placeholder="请输入手机号码" />
        <div class="error-tips" style="display:none"></div>
      </div>
      <div class="pop-register-item">
        <input type="password" name="Pwd" placeholder="请输入密码" />
        <div class="error-tips" style="display:none"></div>
      </div>
      <div class="pop-register-item">
        <input type="text" name="ValidateCode" placeholder="请输入短信验证码" />
        <span class="register-code-button">获取验证码</span>
        <div
          class="error error-tips js-register-tips"
          style="display:none"
        ></div>
      </div>
      <!-- </div> -->
      <div class="pop-register-item" style="display:none">
        <input type="text" name="Source" value="shop" />
      </div>
    </fieldset>
  </form>
  <a
    href="javascript:void(0);"
    class="pop-register-primary-button register-disabled">注册</a>
  <label for="agree" class="auto-login">
    <input type="checkbox"  id="agree" name="agree" value="0" checkbox="false" />注册即代表同意<a href="http://dot.kdfafa.com/agreement" target="_blacnk">《用户协议》</a>
  </label>
  <div class="reg-logon">
      <span class="">
        <i class="f-fl">已有账号？</i><a href="javascript:void(0);" class="f-fl f-cus" id="click_z_login">立刻登录</a>
      </span>
    </div>
</div>

